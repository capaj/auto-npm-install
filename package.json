{
  "name": "auto-npm-install",
  "displayName": "Auto NPM install",
  "description": "A plugin to automatic install a missing npm package",
  "version": "0.0.2",
  "publisher": "moshfeu",
  "engines": {
    "vscode": "^1.6.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onDebug"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.sayHello",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "typescript": "^2.9.2",
    "vscode": "^1.1.21",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moshfeu/auto-npm-install.git"
  },
  "keywords": [
    "vscode",
    "extension"
  ],
  "author": "MoshFeu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moshfeu/auto-npm-install/issues"
  },
  "homepage": "https://github.com/moshfeu/auto-npm-install#readme",
  "icon": "assets/icon.jpg",
  "galleryBanner": {
    "color": "#000",
    "theme": "dark"
  },
  "dependencies": {}
}
